  "bugs": {
    "url": "https://github.com/andreaif/google-search/issues"
  },
  "homepage": "https://github.com/andreaif/google-search#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors-express": "^0.2.2",
    "express": "^4.16.4",
    "mongoose": "^5.5.1",
    "node": "^11.13.0",
    "node-sass": "^4.11.0",
    "router": "^1.3.3"
  },
  "devDependencies": {}
}
